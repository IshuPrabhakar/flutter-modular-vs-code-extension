{
  "icon": "images/icon.jpg",
  "name": "flutter-modular-structure-generator",
  "displayName": "Flutter Modular Structure Generator",
  "description": "Generate modular folder structures for Flutter apps using Riverpod and GoRouter.",
  "version": "1.0.0",
  "publisher": "IshuPrabhakar",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": ["Other", "Snippets"],
  "keywords": [
    "Flutter",
    "Riverpod",
    "GoRouter",
    "Structure",
    "Modular",
    "Boilerplate",
    "Generator",
    "Feature",
    "Architecture"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/IshuPrabhakar/flutter-modular-vs-code-extension.git"
  },
  "bugs": {
    "url": "https://github.com/IshuPrabhakar/flutter-modular-vs-code-extension/issues"
  },
  "homepage": "https://github.com/IshuPrabhakar/flutter-modular-vs-code-extension",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flutter-modular-structure-generator.flutterDirStructure",
        "title": "Generate Folder Structure",
        "category": "Flutter"
      },
      {
        "command": "flutter-modular-structure-generator.flutterFeatureDirStructure",
        "title": "Generate Feature Folder Structure",
        "category": "Flutter"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
